class Solution:
    def getOrder(self, tasks: List[List[int]]) -> List[int]:
#trial2
        start = heappop([x[0] for x in tasks])
        test = defaultdict(int)
        tsk = defaultdict(list)
        for (x,y) in tasks:
            tsk[x-start].append(y)

        for i in range(len(tasks)):
            tsk[i]





#trial1
        tsk = defaultdict(int)
        for (x,y) in tasks:
            tsk[x-start] = y

        ans = []
        #start = min(tsk.keys())
        for i in range(len(tasks)):
            curr = 0
            while curr <= i:
                tsk[curr] -= 1
                if tsk[curr] == 0:
                    ans.append(curr)
                    del tsk[curr]
                curr += 1

        #return ans 
        #return sorted(tsk.items(),key=lambda x:x[1])
        return tsk.items()